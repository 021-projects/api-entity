{
    "name": "021/api-entity",
    "type": "library",
    "description": "Quickly and conveniently interpret JSON data into PHP classes",
    "license": "MIT",
    "authors": [
        {
            "name": "021-projects",
            "email": "devsellio@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "O21\\ApiEntity\\": "src"
        },
        "files": [
            "src/Response.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "illuminate/support": "^7.0|^8.0|^9.0|^10.0|^11.0|^12.0",
        "php": ">=8.1 <8.4",
        "psr/http-message": "^2.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "phpunit/phpunit": "^10.5"
    },
    "scripts": {
        "test": [
            "vendor/bin/phpunit tests"
        ]
    }
}
